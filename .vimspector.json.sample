{
  "configurations": {
    "run": {
      "adapter": "debugpy",
      "default": true,
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/main.py",
        "cwd": "${workspaceRoot}",
        "stopOnEntry": false,
        "python": "./env/bin/python"
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
